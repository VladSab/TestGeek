int[] array = new int[10];
for (int i = 0; i < array.Length; i++)
array[i] = new Random().Next(100, 1000);
Console.WriteLine("[" + string.Join(", ", array) + "]");
int summ = 0;
for (int i = 0; i < array.Length; i ++)
{ 
    if (i % 2 != 1)
        summ = summ + array[i];
}
Console.WriteLine("Cуммa элементов, стоящих на нечётных позициях: " + summ);